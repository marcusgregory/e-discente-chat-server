  "scripts": {
    "build": "tsc --project ./",
    "postinstall": "npm run build",
    "start:dev-debug": "DEBUG=* nodemon src/server.ts",
    "start:dev-fast": "ts-node-dev --respawn --transpile-only src/server.ts",
